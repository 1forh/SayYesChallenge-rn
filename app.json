{
  "expo": {
    "name": "SayYesChallenge",
    "slug": "SayYesChallenge",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "2",
      "supportsTablet": false,
      "userInterfaceStyle": "dark",
      "backgroundColor": "#1C2125",
      "bundleIdentifier": "com.skycatchfire.SayYesChallenge",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library to choose a photo for each day of the challenge."
      }
    },
    "android": {
      "package": "com.skycatchfire.SayYesChallenge",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
